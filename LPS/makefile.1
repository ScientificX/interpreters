
# Standard defs:
OBJS = $(SRCS:.hs=.o)
GHC  = ghc
CC   = gcc
RM   = rm -f

BASICFLAGS = -fglasgow-exts \
	     -fallow-overlapping-instances \
	     -fallow-undecidable-instances

