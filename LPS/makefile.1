	SubType.o \
	Table.o \
	TermB.o \
	TermCMP.o \
	TermD.o \
	TermF.o \
	TermN.o \
	TermP.o \
	TermR.o \
	TermV.o 

mods : $(OBJS)
	$(RM) $@
	$(GHC) -o $@ $(FLAGS) $(OBJS)

### Boilerplate GHC stuff

.SUFFIXES	: .lhs .hs .hi .o

depend :: 	$(SRC)
		$(GHC) -M $(DEPENDFLAGS) $(SRCS)

clean ::
	$(RM) *.o *.hi *.exe *~

%.hi		: %.o
		@:

%.o		: %.hs
		$(RM) $@
		$(GHC) $(FLAGS) -c $< 

